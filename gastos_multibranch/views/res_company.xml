<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_res_company_inherit_ok" model="ir.ui.view">
            <field name="name">view.res.company.inherit.ok</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
		        <xpath expr="//notebook[@colspan='4']" position="inside">
                    <page string="Branch" name="branch_info">
                        <group>
                            <field name="puertoquetzal"/>
                            <field name="santotomas"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
</odoo>